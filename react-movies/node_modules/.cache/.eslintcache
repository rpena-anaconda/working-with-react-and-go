(response) => {\n                if (response.status !== 200) {\n                    setError(true);\n                    setErrorMessage(`Invalid response code: ${response.status}`);\n                    return;\n                }\n\n                return response.json()\n            })\n            .then((json) => {\n                if (json) {\n                    setMovies(json.movies);\n                    setError(false);\n                }\n                setLoading(false)\n            })\n    }, [])\n\n    return (\n        <>\n            <h2>Genre: {genreName}</h2>\n            {!movies &&\n                <p>There are no movies associated to this genre.</p>\n            }\n            {movies && movies.map((movie) => (\n                <Link key={movie.id} to={`/movies/${movie.id}`} className='list-group-item list-group-item-action'>\n                    {movie.title}\n                </Link>\n                // <Movie key={movie.id} movie={movie} />\n            ))}\n        </>\n    );\n}\n\nexport default Genre;\n","/Users/rpena/Sites/udemy/working-with-react-and-go/react-movies/src/components/Movie.js",[],"/Users/rpena/Sites/udemy/working-with-react-and-go/react-movies/src/components/EditMovie.js",["79","80"],"/Users/rpena/Sites/udemy/working-with-react-and-go/react-movies/src/components/form-components/Input.js",[],"/Users/rpena/Sites/udemy/working-with-react-and-go/react-movies/src/components/form-components/Textarea.js",[],"/Users/rpena/Sites/udemy/working-with-react-and-go/react-movies/src/components/form-components/Select.js",[],"/Users/rpena/Sites/udemy/working-with-react-and-go/react-movies/src/components/ui-components/Alert.js",[],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":3,"column":8,"nodeType":"87","messageId":"88","endLine":3,"endColumn":14},{"ruleId":"89","severity":1,"message":"90","line":26,"column":8,"nodeType":"91","endLine":26,"endColumn":10,"suggestions":"9